{
  "schemaVersion": 1,
  "id": "dynamichud",
  "version": "${version}",
  "name": "DynamicHUD",
  "description": "A simple HUD library",
  "authors": [
    "tanishisherewith"
  ],
  "contributors": [
    "LumaaDev"
  ],
  "icon": "assets/DynamicHUD/DynamicHud-logo.png",
  "contact": {
    "repo": "https://github.com/V-Fast/DynamicHUD",
    "issues": "https://github.com/V-Fast/DynamicHUD/issues"
  },
  "license": "All-Rights-Reserved",
  "environment": "client",
  "entrypoints": {
    "client": [
      "com.tanishisherewith.dynamichud.DynamicHUD",
      "com.tanishisherewith.dynamichud.DynamicHUDmod"
    ],
    "modmenu": [
      "com.tanishisherewith.dynamichud.ModMenuIntegration"
    ]
  },
  "depends": {
    "fabricloader": ">=${loader_version}",
    "fabric": "*",
    "minecraft": "~${minecraft_version}"
  },
  "suggests": {
    "modmenu": ">=7.1.0"
  },
  "custom": {
    "modmenu": {
      "badges": [ "library"],
      "update_checker": true
    }
  }
}
